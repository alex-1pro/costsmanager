<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="il.ac.hit.costmanager.model.Expense" table="EXPENSE">
		<id name="id" type="int" column="ID" >
			<generator class="increment"/>
		</id>
		<property name="userId">
			<column name="USERID"/>
		</property>
		<property name="total">
			<column name="TOTAL"/>
		</property>
		<property name="type">
			<column name="TYPE"/>
		</property>
		<property name="month">
			<column name="MONTH"/>
		</property>
		<property name="description">
			<column name="DESCRIPTION"/>
		</property>
	</class>
</hibernate-mapping>